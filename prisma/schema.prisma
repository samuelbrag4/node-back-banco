generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model task {
  id        Int @id @default(autoincrement())
  descricao String
  concluida Boolean @default(false)
  
  criadaEm  DateTime @default(now())

  @@map("tasks")
}